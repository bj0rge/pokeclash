{
  "name": "pokeclash",
  "version": "0.0.1",
  "description": "Website to randomly assign pokemon types to trainers for a tournament",
  "homepage": "https://github.com/bj0rge/pokeclash",
  "author": {
    "name": "Bastien Jorge",
    "email": "bastien.jorge@gmail.com"
  },
  "main": "build/src/server.js",
  "keywords": [
    "Node",
    "Express",
    "Typescript",
    "Sequelize",
    "Pokemon"
  ],
  "scripts": {
    "build": "gulp build",
    "doc": "gulp generate:doc",
    "start": "cross-env NODE_ENV=development gulp watch",
    "start:prod": "cross-env NODE_ENV=production gulp watch",
    "run:test": "cross-env NODE_ENV=test gulp test",
    "tslint": "gulp tslint",
    "prettify": "./node_modules/.bin/tsfmt -r --baseDir ./src",
    "sqlz:migrate": "./node_modules/.bin/sequelize db:migrate",
    "sqlz:undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "sqlz:new": "./node_modules/.bin/sequelize migration:create",
    "genmig": "./node_modules/.bin/sequelize migration:generate --name"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run prettify",
      "git add"
    ]
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "cross-env": "6.0.3",
    "express": "4.17.3",
    "express-boom": "3.0.0",
    "joi": "^14.3.1",
    "morgan": "1.9.1",
    "pg": "7.15.1",
    "pg-hstore": "2.3.3",
    "sequelize": "5.21.3",
    "uuid": "3.3.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "extendify": "1.0.0",
    "glob": "7.1.6",
    "gulp": "4.0.2",
    "gulp-istanbul": "1.1.3",
    "gulp-json-refs": "0.1.1",
    "gulp-mocha": "7.0.2",
    "gulp-nodemon": "2.4.2",
    "gulp-plumber": "1.2.1",
    "gulp-sourcemaps": "2.6.5",
    "gulp-tslint": "8.1.4",
    "gulp-typedoc": "2.2.3",
    "gulp-typescript": "5.0.1",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "remap-istanbul": "0.13.0",
    "rimraf": "3.0.0",
    "sequelize-cli": "5.5.1",
    "tslint": "5.20.1",
    "typedoc": "0.15.5",
    "typescript": "3.7.4",
    "typescript-formatter": "7.2.2"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "license": "SEE LICENSE IN LICENSE.md"
}
